{
  "host": "0.0.0.0",
  "port": 8081,
  "public": "../public/",
  "paginate": {
    "default": 10,
    "max": 50
  },
  "application": {
    "invite_codes": ["Cruddy123!"]
  },
  "authentication": {
    "secret": "",
    "strategies": [
      "jwt",
      "local"
    ],
    "path": "/authentication",
    "service": "users",
    "jwt": {
      "header": {
        "typ": "access"
      },
      "audience": "https://yourdomain.com",
      "subject": "anonymous",
      "issuer": "feathers",
      "algorithm": "HS256",
      "expiresIn": "1d"
    },
    "local": {
      "entity": "user",
      "usernameField": "email",
      "passwordField": "password"
    }
  },
  "nedb": "../data",
  "sqlite": {
    "client": "sqlite3",
    "connection": {
      "filename": "../data/server.sqlite"
    }
  }
}
